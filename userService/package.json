{
  "name": "userservice",
  "scripts": {
    "start": "ts-node-dev ./src/index.ts",
    "build": "tsc",
    "typeorm": "typeorm-ts-node-esm",
    "migration:generate": "npm run typeorm -- -d ./src/config/database.ts migration:generate ./src/migrations/userservice",
    "migration:run": "npm run typeorm -- -d ./src/config/database.ts migration:run"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "http-status-codes": "^2.3.0",
    "mongodb": "^5.4.0",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.20",
    "winston": "^3.13.0",
    "znv": "^0.4.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.7",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5"
  }
}
